#include<stdio.h>
#include<math.h>
/* SOLVE A QURDATIC EQUATION */
void
main ()
{
  int a, b, c, d;
  double root1, root2, root3;
  scanf ("%d %d %d", &a, &b, &c);

  d = b * b - 4 * a * c;

  if (d == 0)
    {
      root3 = (-b) / (2 * a);
      printf ("%lf %lf", root3, root3);
    }
  else if (d > 0)
    {

      root1 = (-b + sqrt (d)) / (2 * a);
      root2 = (-b - sqrt (d)) / (2 * a);
      printf ("%lf %lf", root1, root2);
    }

  else
    {

      printf ("ROOTS ARE IMAGENARY");
    }




}

